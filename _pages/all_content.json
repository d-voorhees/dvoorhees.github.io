---
layout: null
permalink: /all_content.json
---
{
  "content": [
    {% for post in site.posts %}
      {
        "title": {{ post.title | jsonify }},
        "url": "{{ post.url }}",
        "categories": {{ post.categories | jsonify }},
        "introduction": {{ post.introduction | jsonify }}
      }{% unless forloop.last %},{% endunless %}
    {% endfor %}
  ]
}
